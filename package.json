{
  "name": "gatsby-remark-curlie",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "coverage": "nyc report --temp-directory=coverage",
    "test": "$npm_execpath run build && jest --coverage",
    "prepare": "$npm_execpath run clean && $npm_execpath run build && rollup -c",
    "format": "bsrefmt"
  },
  "repository": "https://github.com/arnau/gatsby-remark-curlie",
  "keywords": [
    "bucklescript",
    "reason",
    "gatsby",
    "gatsby-plugin",
    "remark"
  ],
  "author": "Arnau Siches",
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-platform": "^8.0.2",
    "nyc": "^15.1.0",
    "rollup": "^2.22.0",
    "@rollup/plugin-commonjs": "^15.0.1",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "bs-webapi": "^0.19.1",
    "unist-util-visit": "^2.0.3"
  }
}
